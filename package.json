{
  "name": "space",
  "version": "1.0.0",
  "description": "Star Pilot Blues",
  "type": "module",
  "main": "",
  "scripts": {
    "start": "node dist/start.js",
    "build": "tsc\nnpm run build-frontend&\nnpm run build-engine-loader&\nnpm run build-assets&\nwait",
    "build-debug": "tsc && npm run build-frontend-debug && npm run build-engine-loader-debug && npm run build-assets",
    "build-frontend": "node ./builder --minify",
    "build-engine-loader": "esbuild dist/client/engine_loader.js --bundle --minify --outfile=dist/client/dist_engine_loader.js",
    "build-frontend-debug": "esbuild dist/client/frontend.js --bundle --outfile=dist/client/dist_frontend.js",
    "build-engine-loader-debug": "esbuild dist/client/engine_loader.js --bundle --outfile=dist/client/dist_engine_loader.js",
    "build-assets": "npm run build-html && npm run build-img && npm run build-css && npm run build-sfx && npm run build-ico",
    "build-html": "cp src/client/*.html dist/client/",
    "build-img": "cp -r src/client/img dist/client/",
    "build-css": "cp src/client/frontend/*.css dist/client/frontend/",
    "build-sfx": "cp -r src/client/sfx dist/client",
    "build-ico": "cp src/client/favicon.ico dist/client/favicon.ico",
    "prepare": "ts-patch install && typia patch"
  },
  "keywords": [
    "space",
    "cowboy",
    "bebop",
    "online",
    "game"
  ],
  "author": "Spark Cruz <spark.crz@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jsonfile": "^5.0.1",
    "asserts": "^4.0.2",
    "constant": "^1.0.2",
    "esbuild": "^0.25.5",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-vue3": "^0.4.2",
    "events": "^3.3.0",
    "fs": "^0.0.1-security",
    "stream": "^0.0.3",
    "ts-patch": "^3.3.0",
    "typescript": "~5.7.3",
    "util": "^0.12.5"
  },
  "dependencies": {
    "collisions": "^2.0.13",
    "express": "^4.16.3",
    "flatstr": "^1.0.12",
    "long": "^4.0.0",
    "pixi-virtual-joystick": "^0.4.1",
    "pixi.js": "^8.10.1",
    "slow-json-stringify": "^2.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript-json": "^7.6.4",
    "uglify-es": "^3.3.9",
    "vue": "^3.5.16"
  }
}